{
  "name": "mern_typescript",
  "version": "1.0.0",
  "description": "MERN Stack with ES6 and Typescript Boilerplate",
  "main": "server.js",
  "scripts": {
    "start": "node server.js  --exec babel-node",
    "dev": "export $(xargs < .env) && nodemon server.js --exec babel-node",
    "prisma:migrate": "npx prisma migrate dev --name init && npx prisma generate",
    "prisma:seed": "npx prisma db seed --preview-feature"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "2.24.1",
    "bcryptjs": "^2.4.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "nodemon": "^1.17.5",
    "prisma": "^2.26.0"
  }
}
